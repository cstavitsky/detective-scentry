{
    "name": "Santo's SDK Detector",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Finds Sentry SDKs and others",
    // "homepage_url": "https://duo.com/labs",
    "icons": {
      "16": "icons/santo-16.png",
      "32": "icons/santo-32.png",
      "64": "icons/santo-64.png"
    },
    "web_accessible_resources": [
      "js/*",
      "content.js",
      "*.map"
    ],
    "browser_action": {
      "default_title": "Santo SDK Detector",
      "default_popup": "popup.html"
    },
    "default_locale": "en",
    // "background": {
    //   "scripts": [
    //     "js/background.js"
    //   ],
    //   "persistent": true
    // },
    "permissions": [
      "https://*/*",
      "activeTab",
      "tabs",
      "<all_urls>"
    ],
    // "content_security_policy": "default-src 'self';",
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": [
          "inject.js"
        ],
        "all_frames": true
      }
    ]
  }

